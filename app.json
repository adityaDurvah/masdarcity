{
  "expo": {
    "scheme":"com.durvah.masdarcity",
    "name": "masdarcity",
    "slug": "masdarcity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.durvah.masdarcity"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "scss",
        "sass"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "ce8c5510-b888-41a5-ba7d-984c7b65d394"
      }
    },
    "owner": "abhishek.kollipara",
    "plugins": [
      "expo-secure-store"
    ]
  }
}